class Solution {
  public:
    int mod = 1e9+7; 
    int uniquePaths(int n, int m, vector<vector<int>> &grid) {
        // code here
        vector<vector<int>>dp(n, vector<int>(m ,0 )) ; 
        
        dp[0][0] = grid[0][0]; 
        for(int i =1 ; i<m ;i++)
        {
            if(grid[0][i]==0)
            dp[0][i] =0 ; 
            else
            dp[0][i] =dp[0][i-1] ;
        }
        for(int i =1 ; i<n ;i++)
        {
            if(grid[i][0]==0)
            dp[i][0] =0 ; 
            else
            dp[i][0] =dp[i-1][0] ;
        }
        for(int i=1; i<n; i++)
        {
            for(int j =1; j<m; j++)
            {
                if(grid[i][j]==0)
                dp[i][j]=0 ;
                else
                dp[i][j] = (dp[i][j-1]+dp[i-1][j])%mod ;
            }
        }
        return dp[n-1][m-1] ;
    }
};
