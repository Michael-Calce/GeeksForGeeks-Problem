class Solution{
    public:
    //Function to count the frequency of all elements from 1 to N in the array.
    void frequencyCount(vector<int>& arr,int N, int P)
    { 
        P=50000;
         for(int i=0;i<N;i++){
             arr[i]=(P+1)*arr[i];
         }
         for(int i=0;i<N;i++){
             int num=arr[i]/(P+1);
             if(num <= N)
             arr[num-1]=arr[num-1]+1;
         }
         for(int i=0;i<N;i++){
             arr[i]=arr[i]%(P+1);
         }
    }
};
