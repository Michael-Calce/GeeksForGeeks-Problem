class Solution{
public:
    string findLargest(int N, int S){
        // code here
        if(S>9*N || (S==0 && N>1)) return "-1";
        string res="";
        while(S>=9){
            res+="9";
            S-=9;
        }
        while(res.size()!=N){
            res+=S+'0';
            S=0;
        }
        return res;
    }
};
