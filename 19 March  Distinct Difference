class Solution {
  public:
   vector<int> getDistinctDifference(int N, vector<int> &A) {
        unordered_map<int,int>mp1;
        unordered_map<int,int>mp2;
        vector<int>ans;
        for(auto it:A)
        {
            mp1[it]++;
        }
        for(int i=0;i<N;i++)
        {
            mp1[A[i]]--;
            if(mp1[A[i]]==0) mp1.erase(A[i]);
            ans.push_back(mp2.size()-mp1.size());
            mp2[A[i]]++;
        }
        return ans;
    }
};
