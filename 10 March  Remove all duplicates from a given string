class Solution{
public:
 
    bool checkAvail(string temp, char c){
        for(int i=0; i<temp.length(); i++){
            if(temp[i]==c){
                return true;
            }
        }
        return false;
    }
    
string removeDuplicates(string str) {
    // code here
    string temp = "";
    temp+=str[0];
    for(int i=1; i<str.length(); i++){
        if(!checkAvail(temp, str[i])){
            temp+=str[i];
        }
    }
    return temp;
}
};
